{
    "manifest_version": 3,
    "name": "Page Data Fetcher",
    "version": "1.0",
    "description": "Fetch HTML content of the active tab",
    "permissions": ["activeTab", "scripting", "storage"],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ]
}
  